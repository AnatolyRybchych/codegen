#include <stdlib.h>

typedef struct Array${Type} Array${Type};

struct Array${Type}{
    size_t capacity;
    size_t count;
    ${Type} elements[];
};


$eval{
    expression
}


$set{A=B}      => {"A": "B"}
$set{A=B}      => {"A": "B"}
$set{A=}       => {"A": ""}
$set{A}        => {"A": ""}
$set{A= B }"   => {"A": " B "}
